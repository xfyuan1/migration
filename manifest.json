{
  "manifest_version": 2,
  "name": "Coursera Migrations Modularization Extension",
  "description": "This extension will check the non-video or quiz boxes",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jQuery.js"]
    }
  ],
  "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}